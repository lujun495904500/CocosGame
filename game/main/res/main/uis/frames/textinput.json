{
    "meta":"TextInput"
    ,"name":"textinput"
    ,"params":{
        
    }
    ,"csb":"$(respath)/main/cocosstudio/TextInput.csb"
    ,"widgets":{
		"lb_message":{
			"source":"lb_message",
			"type":"label",
			"config":{
				"font":{
					"name":"font_16"
				}
            }
		}
		,"wg_words":{
            "source":"wg_words",
            "type":"table",
            "config":{
                "itemsize":[16,16],
                "selectable":true,
                "itemrows":6,
                "itemcolumns":6,
				"itemwidget":{
					"type":"label"
				}
            }
        }
		,"wg_functions":{
            "source":"wg_functions",
            "type":"table",
            "config":{
                "itemsize":[32,16],
                "selectable":true,
                "itemrows":5,
                "itemcolumns":1,
				"itemwidget":{
					"type":"label"
				}
            },
			"params":{
				"items":[
					{
						"label":"删除",
						"type":"D"
					},{
						"label":"小写",
						"type":"L"
					},{
						"label":"大写",
						"type":"U"
					},{
						"label":"数字",
						"type":"N"
					},{
						"label":"符号",
						"type":"S"
					}
				]
			}
        }
		,"wg_select":{
            "source":"wg_select",
            "type":"table",
            "config":{
                "itemsize":[32,16],
                "selectable":true,
                "itemrows":2,
                "itemcolumns":1,
				"itemwidget":{
					"type":"label"
				}
            },
			"params":{
				"items":[
					{
						"label":"返回",
						"type":"R"
					},{
						"label":"确定",
						"type":"E"
					}
				]
			}
        }
    }
	,"bindings":{
		"eb_wrect":"pl_inwords/eb_wrect"
	}
}